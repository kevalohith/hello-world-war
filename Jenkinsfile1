@Library('shared-library-prac@main') _
pipeline {
    agent none
    environment {
        ARTIFACTORY_CREDENTIALS = credentials('jfrog_artifactory_credentials')
    }
        
    stages 
    {
        stage('checkout') {
            agent { label 'build-server'}
            steps {
              checkoutstage-k()
            }
        }
         stage('build') {
             agent { label 'build-server'}
            steps {
              buildstage-k()
                }
            }
         stage('publish') { 
             agent { label 'build-server'}
            steps {
              publishstage-k()
            }    
        }
       stage('deploy') {
           agent { label 'deployer'}
           steps {
            deploystage-k(env.BUILD_NUMBER, '54.163.15.154')
           }
       }
    }   
}
